pipeline {
	agent any
	stages {
		stage ('Compile Stage') {
			steps {
				bat 'gradle compileJava'
			}
		
		}
		stage ('Test') {
		
			steps {
				bat 'gradle test'
			}
		}
		stage ('Build') {
		
			steps {
				bat 'gradle build'
			}
		}
		stage ('Build Docker Image')
		{
			steps {
				step ('deleting image')
				{
				bat docker rm employeecontroller
				}
			}
				
	}
}